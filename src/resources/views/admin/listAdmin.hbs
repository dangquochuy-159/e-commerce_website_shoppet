<style>
    .avt-admin img {
        width: 50px;
        height: 50px;
    }
</style>
{{> test}}
<table class="table mt-4">
    <thead>
        <tr>
            <th class="text-center" scope="col">ID</th>
            <th class="text-center" scope="col">Tên tài khoản</th>
            <th class="text-center" scope="col">Tên đăng nhập</th>
            <th class="text-center" scope="col">Ảnh</th>
        </tr>
    </thead>
    <tbody>
        {{#each admin}}
        <tr>
            <th class="text-center" scope="row">{{this.admin_ID}}</th>
            <td class="text-center">{{this.name}}</td>
            <td class="text-center">{{this.user_name}}</td>
            <td class="text-center">
                <div class="avt-admin">
                    <img src="{{this.image}}" alt="">
                </div>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>